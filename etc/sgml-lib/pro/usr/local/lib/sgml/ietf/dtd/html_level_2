<!ELEMENT INPUT  - O  EMPTY  --<Title>INPUT-- >

<!ATTLIST INPUT
               --<Title>html - Attribute List-029--
               
               TYPE (TEXT,PASSWORD,CHECKBOX,RADIO,SUBMIT,RESET,IMAGE,
               HIDDEN)  "TEXT"
               --<Title>TYPE--
               
               NAME  CDATA    #IMPLIED 
               --<Title>NAME--
               
               VALUE  CDATA    #IMPLIED 
               --<Title>VALUE--
               
               SRC  CDATA    #IMPLIED 
               --<Title>SRC-002--
               
               CHECKED (CHECKED)   #IMPLIED 
               --<Title>CHECKED--
               
               SIZE  CDATA    #IMPLIED 
               --<Title>SIZE--
               
               MAXLENGTH  NUMBER    #IMPLIED 
               --<Title>MAXLENGTH--
               
