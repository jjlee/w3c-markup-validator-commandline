               --<Title>SDAPREF-012--
                >

<!ELEMENT SELECT  - - (OPTION+) -(INPUT,SELECT,TEXTAREA) --<Title>SELECT-- >

<!ATTLIST SELECT
               --<Title>html - Attribute List-030--
               
               NAME  CDATA    #REQUIRED 
               --<Title>NAME-002--
               
               SIZE  NUMBER    #IMPLIED 
               --<Title>SIZE-002--
               
               MULTIPLE (MULTIPLE)   #IMPLIED 
               --<Title>MULTIPLE--
               
               SDAFORM  CDATA    #FIXED "List"
               --<Title>SDAFORM-011--
               
               SDAPREF  CDATA    #FIXED 
               "<LHead>Select #AttVal(Multiple)</LHead>"
               
               --<Title>SDAPREF-013--
                >

<!ELEMENT TEXTAREA  - - (#PCDATA) -(INPUT,SELECT,TEXTAREA) --<Title>TEXTAREA-- >

<!ATTLIST TEXTAREA
               --<Title>html - Attribute List-032--
               
               NAME  CDATA    #REQUIRED 
               --<Title>NAME-002--
               
               ROWS  NUMBER    #REQUIRED 
               --<Title>ROWS--
               
               COLS  NUMBER    #REQUIRED 
               --<Title>COLS--
               
               SDAFORM  CDATA    #FIXED "Para"
               --<Title>SDAFORM-004--
               
               SDAPREF  CDATA    #FIXED "Input Text -- #AttVal(Name): "
               
               --<Title>SDAPREF-015--
                >

<!ELEMENT OPTION  - O (#PCDATA) --<Title>OPTION-- >

<!ATTLIST OPTION
               --<Title>html - Attribute List-031--
               
               SELECTED (SELECTED)   #IMPLIED 
               --<Title>SELECTED--
               
               VALUE  CDATA    #IMPLIED 
               --<Title>VALUE--
               
               SDAFORM  CDATA    #FIXED "LItem"
               --<Title>SDAFORM-013--
               
               SDAPREF  CDATA    #FIXED 
               "Option: #AttVal(Value) #AttVal(Selected)"
               
               --<Title>SDAPREF-014--
                >

<!ENTITY % ISOlat1   PUBLIC 
               "ISO 8879-1986//ENTITIES Added Latin 1//EN//HTML"   
               --<Title>ISOlat1--
                >

