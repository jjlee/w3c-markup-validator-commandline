               --<Title>WIDTH--
               
               SDAFORM  CDATA    #FIXED "Lit"
               --<Title>SDAFORM--
                >

<!ELEMENT XMP  - -  CDATA  --<Title>XMP-- >

<!ATTLIST XMP
               --<Title>html - Attribute List-015--
               
               SDAFORM  CDATA    #FIXED "Lit"
               --<Title>SDAFORM--
               
               SDAPREF  CDATA    #FIXED "Example: "
               --<Title>SDAPREF-005--
                >

<!ELEMENT LISTING  - -  CDATA  --<Title>LISTING-- >

